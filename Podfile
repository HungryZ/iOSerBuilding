platform :ios, '8.0'
#use_frameworks!个别需要用到它，比如reactiveCocoa 或Swift

# ruby grammar
targetsArray = [‘iOSerBuilding’]
targetsArray.each do |t|
    target t do
        pod 'AFNetworking'
        pod 'Masonry'
        pod 'MJRefresh'
        pod 'MJExtension'
        pod 'CYLTabBarController'
    end
    
end
